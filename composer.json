	"type": "phpbb-extension",
	"description": "Reply Template Extension for phpBB 3.2",
	"homepage": "https://github.com/marttiphpbb/phpbb-ext-replytemplate",
	"version": "0.1.0",
	"time": "2018-04-19",
	"license": "GPL-2.0",
	"authors": [{
			"name": "martti",
			"email": "info@martti.be",
			"homepage": "https://github.com/marttiphpbb/",
			"role": "Developer"
		}],
	"require": {
		"php": ">=7"
	},
	"require-dev": {
		"phpbb/epv": "dev-master"
